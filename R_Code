  geom_errorbar(aes(x = Treatment_Stage,
                    ymin = mean,
                    ymax = mean),
                width = .3) +
  geom_errorbar(aes(x = Treatment_Stage,
                    ymin = mean - se,
                    ymax = mean + se),
                width = .5) +
  labs(x = "Treatment Stage",
       y = "Relative ARG Expression") +
  theme_classic(base_size = 12)
ggsave("Figures/Tetracycline9.png", width = 6, height = 4)

